{
  "name": "booking-service",
  "description": "> Project description",
  "author": "Jordan Pagni",
  "contrirbutors": [
    {
      "name": "Doruk Gurleyen",
      "url": "https://github.com/kzlsakal"
    }
  ],
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "seed": "node database/seedData.js",
    "generate": "node server/database/generate.js",
    "start:dev": "nodemon server/index.js -w server -w database",
    "build:dev": "webpack --mode production",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^3.4.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.2.0",
    "eslint-config-hackreactor": "https://github.com/reactorcore/eslint-config-hackreactor.git",
    "faker": "^4.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.2.1",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.3",
    "style-loader": "^1.2.1",
    "ttf-loader": "^1.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/client"
    ],
    "moduleNameMapper": {
      "\\.(png|eot|ttf|woff|woff2)$": "<rootDir>/spec/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/spec/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "<rootDir>/server/*.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testRegex": "((\\.|/*.)(spec))\\.js?$",
    "setupFilesAfterEnv": [
      "<rootDir>/spec/jest.config.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "version": "1.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrr47-fec8-webber/booking-service.git"
  },
  "bugs": {
    "url": "https://github.com/hrr47-fec8-webber/booking-service/issues"
  },
  "homepage": "https://github.com/hrr47-fec8-webber/booking-service#readme"
}
